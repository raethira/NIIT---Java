class staticdemo2 
{
	static int add(int i,int j)
	{
		return i+j;
	}
	static float add1(float k,float j)
	{
		return k-j;
	}
	void dis()
	{
		System.out.println("gaedjd");
	}
	static void add()
	{
		System.out.println("wrwfrwtw:");
	}
	public static void main(String[] args) 
	{
		System.out.println(staticdemo2.add(23,56));
		System.out.println(staticdemo2.add1(23f,65f));
		staticdemo2.add();
		staticdemo2 s=new staticdemo2();
		s.dis();
		System.out.println("Hello World!");
	}
}
