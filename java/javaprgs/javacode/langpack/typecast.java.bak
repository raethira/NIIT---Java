class typecast 
{
	public static void main(String[] args) 
	{
		byte b;
		int i=249;
		double j=874.6;
		System.out.println("int into byte");
		b=(byte)i;
		System.out.println(b);
		System.out.println("float into byte");
		b=(byte)j;
		System.out.println(b);
		System.out.println("float into int");
		i=(int)j;
		System.out.println(i);
	}
}
