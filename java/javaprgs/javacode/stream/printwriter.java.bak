import java.io.*;
class printwriter
{
	public static void main(String[] args) 
	{	 
		PrintWriter pw=null;
		try
		{
			FileWriter fw=new FileWriter("badrun.txt");
			pw=new PrintWriter(fw);
		}
		catch(Exception e)
		{
			System.out.println(e);
		}
		pw.print("kathir\t");
		pw.print(24);
		pw.println();
		pw.print("badrun\t");
		pw.print(20);
		pw.close();

			System.out.println("Hello World!");
	}
}
